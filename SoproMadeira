public class SoproMadeira extends Sopro {

    @Override
    public void tocar() {
        System.out.println("tocando instrumento sopro (madeira)");
    }

    @Override
    public void nome() {
        System.out.println("SoproMadeira");
    }

    @Override
    public void afinar() {
        System.out.println("afinando instrumento sopro (madeira)");
    }
}
